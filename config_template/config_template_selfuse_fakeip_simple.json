{
    "log": {
        "disabled": false,
        "level": "error",
        "timestamp": true
    },
    "dns": {
        "servers": [
            {
                "tag": "dns_proxy",
                "address": "https://dns.cloudflare.com/dns-query",
                "address_resolver": "dns_resolver",
                "strategy": "prefer_ipv4",
                "detour": "proxy"
            },
            {
                "tag": "dns_direct",
                "address": "https://doh.pub/dns-query",
                "address_resolver": "dns_resolver",
                "strategy": "prefer_ipv4",
                "detour": "direct"
            },            
            {
                "tag": "dns_block",
                "address": "quic://dns.adguard.com",
                "address_resolver": "dns_resolver",
                "strategy": "prefer_ipv4",
                "detour": "block"
            },
            {
                "tag": "dns_resolver",
                "address": "101.226.4.6",
                "strategy": "ipv4_only",
                "detour": "direct"
            },
            {   "tag": "dns_fakeip",
                "address": "fakeip"
            }
        ],
        "rules": [
            {
                "rule_set": "geosite-category-ads-all",
                "server": "dns_block"
            },
            {
                "outbound": "any",
                "server": "dns_resolver"
            },
            {   "clash_mode": "Direct",
                "server": "dns_direct"
            },
            {   "clash_mode": "Global",
                "server": "dns_proxy"
            },
            {
                "rule_set": "geosite-geolocation-cn",
                "server": "dns_direct"
            },            
            {
                "type": "logical",
                "mode": "and",
                "rules": [
                    {
                        "rule_set": "geosite-geolocation-!cn",
                        "invert": true
                    },
                    {
                        "rule_set": "geoip-cn"
                    }
                ],
                "server": "dns_proxy",          
                "client_subnet": "114.114.114.114/24"
            },
            {
                "query_type": ["A", "AAAA"],
                "server": "dns_fakeip"               
            }
        ],
        "final": "dns_proxy",
        "strategy": "prefer_ipv4",       
        "client_subnet": "114.114.114.114/24",
        "fakeip": {
            "enabled": true,
            "inet4_range": "198.18.0.0/15",
            "inet6_range": "fc00::/18"
        }
    },
    "inbounds": [
        {
            "type": "tun",
            "tag": "tun-in",
            "address": ["172.18.0.1/30", "fdfe:dcba:9876::1/126"],
            "mtu": 1492,
            "gso": false,
            "auto_route": true,
            "strict_route": true,
            "route_address": ["0.0.0.0/1", "128.0.0.0/1", "::/1", "8000::/1"],
            "route_exclude_address": ["192.168.0.0/16", "fc00::/7"],
            "endpoint_independent_nat": false,
            "udp_timeout": "5m",
            "stack": "mixed",
            "exclude_package": [
                "com.android.captiveportallogin",
                "com.dianchou.dcw",
                "com.jaemobird.mutongji",
                "com.MobileTicket",
                "com.tencent.mm",
                "com.tencent.mobileqq",
                "com.miui.cloudservice",
                "com.miui.weather2"                
            ],
            "platform": {
                "http_proxy": {
                    "enabled": true,
                    "server": "127.0.0.1",
                    "server_port": 7890
                }
            },
            "sniff": true,
            "sniff_override_destination": true
        },
        {
            "type": "mixed",
            "tag": "mixed-in",
            "listen": "0.0.0.0",
            "listen_port": 7890,
            "users": [],
            "sniff": true,
            "sniff_override_destination": true
        }
    ],
    "outbounds": [
        {
            "tag": "proxy",
            "type": "selector",
            "outbounds": ["auto", "direct", "{all}"],
            "default": "auto",
            "interrupt_exist_connections": true
        },
        {
            "tag": "auto",
            "type": "urltest",
            "outbounds": ["{all}"],
            "filter": [{"action": "exclude", "keywords": ["网站|地址|剩余|过期|时间|有效"]}],
            "url": "https://www.gstatic.com/generate_204",
            "interval": "3m",
            "tolerance": 50,
            "idle_timeout": "30m",
            "interrupt_exist_connections": true
        },
        {
             "tag": "AdBlock",
             "type": "selector",
             "outbounds": ["block", "direct"]
        },
        {
            "tag": "Others",
            "type": "selector",
            "outbounds": ["{all}", "proxy"],
            "filter": [{"action": "exclude", "keywords": ["🇭🇰|HK|hk|香港|香|🇹🇼|TW|tw|台湾|台|🇸🇬|SG|sg|新加坡|狮|🇯🇵|JP|jp|日本|日|🇺🇸|US|us|美国|美|🇰🇷|KR|kr|韩国"]}]
        },
        {
            "type": "direct",
            "tag": "direct"
        },
        {
            "type": "block",
            "tag": "block"
        },
        {
            "type": "dns",
            "tag": "dns-out"
        }
    ],
    "route": {
        "rules": [
            {
                "type": "logical",
                "mode": "or",
                "rules": [
                    {
                        "protocol": "dns"
                    },
                    {
                        "port": 53
                    }
                ],
                "outbound": "dns-out"
            },
            {
                "ip_is_private": true,
                "outbound": "direct"
            },
            {
                "clash_mode": "Direct",
                "outbound": "direct"
            },
            {
                "clash_mode": "Global",
                "outbound": "proxy"
            },
            {
                "type": "logical",
                "mode": "or",
                "rules": [
                    {
                        "port": 853
                    },
                    {
                        "network": "udp",
                        "port": 443
                    },
                    {
                        "protocol": "stun"
                    }
                ],
                "outbound": "block"
            },
            {
                "rule_set": "geosite-category-ads-all",
                "outbound": "block"
            },
            {
                "rule_set": ["geoip-cn", "geosite-geolocation-cn"],
                "outbound": "direct"
            }
        ],
        "final": "proxy",
        "auto_detect_interface": true,
        "rule_set": [
            {
                "tag": "geosite-category-ads-all",
                "type": "remote",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/lyc8503/sing-box-rules/rule-set-geosite/geosite-category-ads-all.srs",
                "download_detour": "auto",
                "update_interval": "1d"
            },
            {
                "tag": "geosite-geolocation-cn",
                "type": "remote",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/lyc8503/sing-box-rules/rule-set-geosite/geosite-geolocation-cn.srs",
                "download_detour": "auto",
                "update_interval": "1d"
            },
            {
                "tag": "geosite-geolocation-!cn",
                "type": "remote",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/lyc8503/sing-box-rules/rule-set-geosite/geosite-geolocation-!cn.srs",
                "download_detour": "auto",
                "update_interval": "1d"
            },            
            {
                "tag": "geoip-cn",
                "type": "remote",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/lyc8503/sing-box-rules/rule-set-geoip/geoip-cn.srs",
                "download_detour": "auto",
                "update_interval": "1d"
            }
        ]
    },
    "experimental": {
        "cache_file": {
            "enabled": true,
            "path": "cache.db",
            "store_fakeip": true,
            "store_rdrc": true,
            "rdrc_timeout": "7d"
        },
        "clash_api": {
            "external_controller": "127.0.0.1:9090",
            "external_ui": "ui",
            "external_ui_download_url": "https://github.com/MetaCubeX/metacubexd/archive/refs/heads/gh-pages.zip",
            "external_ui_download_detour": "direct",
            "secret": "",
            "default_mode": "Rule",
            "access_control_allow_origin": ["http://127.0.0.1", "http://yacd.haishan.me"],
            "access_control_allow_private_network": true
        }
    }
}
