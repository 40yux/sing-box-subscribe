    "route": {
        "rules": [
            {
                "inbound": ["tun-in", "mixed-in"],
                "action": "sniff",
                "sniffer": ["http", "tls", "quic", "stun", "dns", "bittorrent", "dtls", "ssh", "rdp"]
            },
            {
                "type": "logical",
                "mode": "or",
                "rules": [
                    {
                        "port": 53
                    },
                    {
                         "protocol": "dns"
                    }
                ],
                "action": "hijack-dns"
            },
            {
                "rule_set": "geosite-category-ads-all",
                "action": "reject",
                "method": "default",
                "no_drop": false
            },
            {
                "ip_is_private": true,
                "action": "route",
                "outbound": "direct"
            },
            {
                "clash_mode": "Direct",
                "action": "route",
                "outbound": "direct"
            },
            {
                "clash_mode": "Global",
                "action": "route",
                "outbound": "proxy"
            },
            {
                "type": "logical",
                "mode": "or",
                "rules": [
                    {
                        "network": "tcp",
                        "port": 853
                    },
                    {
                        "network": "udp",
                        "port": 443
                    },
                    {
                        "protocol": "stun"
                    }
                ],
                "action": "reject"
            },
            {
                "rule_set": "geosite-geolocation-!cn",
                "action": "route",
                "outbound": "proxy"
            },
            {
                "rule_set": ["geoip-cn", "geosite-geolocation-cn"],
                "action": "route",
                "outbound": "direct"
            }
        ],
        "default_domain_resolver": {
            "server": "dns_direct",
            "rewrite_tll": 60,
            "client_subnet": "1.1.1.1"
        },        
        "final": "proxy",
        "auto_detect_interface": true,
        "rule_set": [
            {
                "tag": "geosite-category-ads-all",
                "type": "remote",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/lyc8503/sing-box-rules/refs/heads/rule-set-geosite/geosite-category-ads-all.srs",
                "download_detour": "auto",
                "update_interval": "1d"
            },
            {
                "tag": "geosite-geolocation-cn",
                "type": "remote",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/lyc8503/sing-box-rules/refs/heads/rule-set-geosite/geosite-geolocation-cn.srs",
                "download_detour": "auto",
                "update_interval": "1d"
            },
            {
                "tag": "geosite-geolocation-!cn",
                "type": "remote",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/lyc8503/sing-box-rules/refs/heads/rule-set-geosite/geosite-geolocation-!cn.srs",
                "download_detour": "auto",
