            "outbounds": ["{all}", "proxy"],
            "filter": [{"action": "exclude", "keywords": ["ğŸ‡­ğŸ‡°|HK|hk|é¦™æ¸¯|é¦™|ğŸ‡¹ğŸ‡¼|TW|tw|å°æ¹¾|å°|ğŸ‡¸ğŸ‡¬|SG|sg|æ–°åŠ å¡|ç‹®|ğŸ‡¯ğŸ‡µ|JP|jp|æ—¥æœ¬|æ—¥|ğŸ‡ºğŸ‡¸|US|us|ç¾å›½|ç¾|ğŸ‡°ğŸ‡·|KR|kr|éŸ©å›½"]}]
