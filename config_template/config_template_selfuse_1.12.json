                    {
                        "rule_set": "geosite-geolocation-!cn",
                        "invert": true
                    },
                    {
                        "rule_set": "geoip-cn"
                    }
                ],
                "action": "route",
                "server": "dns_proxy",
                "client_subnet": "114.114.114.114/24"
            },
            {
                "query_type": ["A", "AAAA"],
                "action": "route",
                "server": "dns_fakeip"
            }
        ],
        "final": "dns_proxy",
        "strategy": "prefer_ipv4"
    },
    "inbounds": [
