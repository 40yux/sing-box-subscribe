        ],
        "rules": [
            {   
                "clash_mode": "Direct",
                "action": "route",
                "server": "dns_direct"
            },
            {   "clash_mode": "Global",
                "action": "route",
                "server": "dns_proxy"
            },
            {
                "rule_set": "geosite-geolocation-cn",
                "action": "route",
                "server": "dns_direct"
            },
            {
                "type": "logical",
                "mode": "and",
                "rules": [
                    {
                        "rule_set": "geosite-geolocation-!cn",
                        "invert": true
                    },
                    {
                        "rule_set": "geoip-cn"
                    }
                ],
                "action": "route",
                "server": "dns_proxy",
                "client_subnet": "114.114.114.114/24"
            },
            {
                "query_type": ["A", "AAAA"],
                "action": "route",
                "server": "dns_fakeip"
            }
        ],
        "final": "dns_proxy",
        "strategy": "prefer_ipv4"
    },
    "ntp": {
        "enabled": true,
        "server": "time.android.com",
        "server_port": 123,
        "detour": "direct"
    },
    "inbounds": [
