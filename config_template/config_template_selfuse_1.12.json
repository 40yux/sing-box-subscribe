{
    "log": {
        "disabled": false,
        "level": "debug",
        "timestamp": true
    },
    "dns": {
        "servers": [
            {
                "type": "https",
                "tag": "dns_proxy",
                "server": "cloudflare-dns.com",
                "domain_resolver": "dns_cloudflare",
                "detour": "proxy"
            },
            {
                "type": "udp",
                "tag": "dns_cloudflare",
                "server": "1.1.1.1",
