                "cn.gov.tax.its",
                "com.MobileTicket",
                "com.eg.android.AlipayGphone",
                "com.huawei.health",
                "com.dragon.read"
            ],
            "platform": {
                "http_proxy": {
                    "enabled": true,
                    "server": "127.0.0.1",
                    "server_port": 8080
                }
            }
        },
        {
            "type": "mixed",
            "tag": "mixed-in",
            "listen": "127.0.0.0",
            "listen_port": 8080
        }
    ],
    "outbounds": [
        {
            "tag": "proxy",
            "type": "selector",
            "outbounds": ["auto", "direct", "{all}"],
            "default": "auto",
            "interrupt_exist_connections": true
        },
        {
            "tag": "auto",
            "type": "urltest",
            "outbounds": ["{all}"],
            "filter": [{"action": "exclude", "keywords": ["ç½‘ç«™|åœ°å€|å‰©ä½™|è¿‡æœŸ|æ—¶é—´|æœ‰æ•ˆ"]}],
