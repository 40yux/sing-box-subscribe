{
    "log": {
        "disabled": false,
        "level": "info",
        "timestamp": true
    },
    "dns": {
        "servers": [
            {
                "type": "https",
                "tag": "dns_proxy",
                "server": "dns.cloudflare.com",
                "server_port": 443,
                "domain_resolver": "dns_resolver"
            },
            {
                "type": "https",
                "tag": "dns_direct",
                "server": "dns.alidns.com",
                "server_port": 443,
                "domain_resolver": "dns_resolver"
            },
            {
                "type": "udp",
                "tag": "dns_resolver",
                "server": "101.226.4.6",
                "server_port": 53
            },
            {
                "type": "fakeip",
                "tag": "dns_fakeip",
                "inet4_range": "198.18.0.0/15",
                "inet6_range": "fc00::/18"

            },
            {
                "type": "tailscale",
                "tag": "ts",
                "endpoint": "ts-ep"
            }
        ],
        "rules": [
            {
                "ip_accept_any": true,
                "server": "ts"
            },
            {   
                "clash_mode": "Direct",
                "action": "route",
                "server": "dns_direct"
            },
            {   "clash_mode": "Global",
                "action": "route",
                "server": "dns_proxy"
            },
            {
                "rule_set": "geosite-geolocation-cn",
                "action": "route",
                "server": "dns_direct"
            },
            {
                "type": "logical",
                "mode": "and",
                "rules": [
                    {
                        "rule_set": "geosite-geolocation-!cn",
                        "invert": true
                    },
                    {
                        "rule_set": "geoip-cn"
                    }
                ],
                "action": "route",
                "server": "dns_proxy",
                "client_subnet": "114.114.114.114/24"
            },
            {
                "query_type": ["A", "AAAA"],
                "action": "route",
                "server": "dns_fakeip"
            }
        ],
        "final": "dns_proxy",
        "strategy": "prefer_ipv4"
    },
    "inbounds": [
        {
            "type": "tun",
            "tag": "tun-in",
            "interface_name": "tun0",
            "address": ["172.18.0.1/30", "fdfe:dcba:9876::1/126"],
            "mtu": 1492,
            "auto_route": true,
            "strict_route": true,
            "route_address": ["0.0.0.0/1", "128.0.0.0/1", "::/1", "8000::/1"],
            "route_exclude_address": ["192.168.0.0/16", "fc00::/7"],
            "endpoint_independent_nat": false,
            "stack": "system",
            "exclude_package": [
                "com.android.captiveportallogin",
                "com.dianchou.dcw",
                "com.jaemobird.mutongji",
                "com.MobileTicket",
                "com.tencent.mm",
                "com.tencent.mobileqq",
                "com.miui.cloudservice",
                "com.miui.weather2",
                "com.xiaomi.market",
                "cn.gov.tax.its"
            ],
            "platform": {
                "http_proxy": {
                    "enabled": true,
                    "server": "127.0.0.1",
                    "server_port": 7890
                }
            }
        },
        {
            "type": "mixed",
            "tag": "mixed-in",
            "listen": "0.0.0.0",
            "listen_port": 7890,
            "users": []
        }
    ],
    "outbounds": [
        {
            "tag": "proxy",
            "type": "selector",
            "outbounds": ["auto", "direct", "{all}"],
            "default": "auto",
            "interrupt_exist_connections": true
        },
        {
            "tag": "auto",
            "type": "urltest",
            "outbounds": ["{all}"],
            "filter": [{"action": "exclude", "keywords": ["网站|地址|剩余|过期|时间|有效"]}],
            "url": "https://www.gstatic.com/generate_204",
            "interval": "30s",
            "tolerance": 50,
            "idle_timeout": "30m",
            "interrupt_exist_connections": true
        },
        {
            "tag": "Others",
            "type": "selector",
            "outbounds": ["{all}", "proxy"],
            "filter": [{"action": "exclude", "keywords": ["🇭🇰|HK|hk|香港|香|🇹🇼|TW|tw|台湾|台|🇸🇬|SG|sg|新加坡|狮|🇯🇵|JP|jp|日本|日|🇺🇸|US|us|美国|美|🇰🇷|KR|kr|韩国"]}]
        },
        {
            "type": "direct",
            "tag": "direct"
        }
    ],
    "route": {
        "rules": [
            {
                "inbound": "in",
                "action": "resolve",
                "strategy": "prefer_ipv4"
            },
            {
                "inbound": ["tun-in","mixed-in"],
                "action": "sniff",
                "timeout": "1s"
            },
            {
                "type": "logical",
                "mode": "or",
                "rules": [
                    {
                        "port": 53
                    },
                    {
                         "protocol": "dns"
                    }
                ],               
                "action": "hijack-dns"
            },
            {
                "ip_is_private": true,
                "action": "route",
                "outbound": "direct"
            },
            {
                "clash_mode": "Direct",
                "action": "route",
                "outbound": "direct"
            },
            {
                "clash_mode": "Global",
                "action": "route",
                "outbound": "proxy"
            },
            {
                "rule_set": ["geoip-cn", "geosite-geolocation-cn"],
                "action": "route",
                "outbound": "direct"
            }
        ],
        "default_domain_resolver":
        {
            "server": "dns_resolver",
            "rewrite_tll": 60,
            "client_subnet": "1.1.1.1"
        },
        "final": "proxy",
        "auto_detect_interface": true,
        "rule_set": [
            {
                "tag": "geosite-geolocation-cn",
                "type": "remote",
                "format": "binary",
                "url": "https://github.com/lyc8503/sing-box-rules/raw/refs/heads/rule-set-geosite/geosite-geolocation-cn.srs",
                "download_detour": "auto",
                "update_interval": "1d"
            },
            {
                "tag": "geosite-geolocation-!cn",
                "type": "remote",
                "format": "binary",
                "url": "https://github.com/lyc8503/sing-box-rules/raw/refs/heads/rule-set-geosite/geosite-geolocation-!cn.srs",
                "download_detour": "auto",
                "update_interval": "1d"
            },
            {
                "tag": "geoip-cn",
                "type": "remote",
                "format": "binary",
                "url": "https://github.com/lyc8503/sing-box-rules/raw/refs/heads/rule-set-geoip/geoip-cn.srs",
                "download_detour": "auto",
                "update_interval": "1d"
            }
        ]
    },
    "experimental": {
        "cache_file": {
            "enabled": true,
            "path": "cache.db",
            "store_fakeip": true,
            "store_rdrc": true,
            "rdrc_timeout": "7d"
        },
        "clash_api": {
            "external_controller": "127.0.0.1:9090",
            "external_ui": "ui",
            "external_ui_download_url": "https://github.com/MetaCubeX/metacubexd/archive/refs/heads/gh-pages.zip",
            "external_ui_download_detour": "direct",
            "secret": "",
            "default_mode": "Rule"
        }
    }
}
