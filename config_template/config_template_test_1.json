{
    "log": {
        "disabled": false,
        "level": "info",
        "timestamp": true
    },
    "dns": {
        "servers": [
            {
                "type": "https",
                "tag": "dns_proxy",
                "server": "https://1.1.1.1/dns-query",
                "domain_resolver": "direct"
            },
            {
                "type": "https",
                "tag": "dns_direct",
                "server": "https://dns.alidns.com/dns-query",
                "domain_resolver": "direct"
            },
            {
                "type": "udp",
                "tag": "dns_resolver",
                "server": "223.5.5.5",
                "server_port": 53
            },
            {
                "type": "fakeip",
                "tag": "dns_fakeip",
                "inet4_range": "172.16.0.0/12",
                "inet6_range": "fd00::/8"

            },
            {
                "type": "tailscale",
                "tag": "ts",
                "endpoint": "ts-ep"
            }
        ],
        "rules": [
            {   
                "clash_mode": "Direct",
                "server": "dns_direct"
            },
            {   "clash_mode": "Global",
                "server": "dns_proxy"
            },
            {
                "rule_set": "geosite-category-ads-all",
                "action": "reject",
                "method": "default",
                "no_drop": false
            },
            {
                "rule_set": "geosite-geolocation-cn",
                "server": "dns_direct"
            },
            {
                "type": "logical",
                "mode": "and",
                "rules": [
                    {
                        "rule_set": "geosite-geolocation-!cn",
                        "invert": true
                    },
                    {
                        "rule_set": "geoip-cn"
                    }
                ],
                "server": "dns_proxy",
                "client_subnet": "114.114.114.114/24"
            },
            {
                "query_type": ["A", "AAAA","MX", "TXT"],
                "server": "dns_fakeip"
            },
            {
                "ip_accept_any": true,
                "server": "ts"
            }
        ],
        "final": "dns_proxy",
        "strategy": "prefer_ipv4"
    },
    "endpoints": [
        {
            "type": "tailscale",
            "tag": "ts-ep",
            "state_directory": "",
            "auth_key": "tskey-auth-xxxxxxxxx",
            "control_url": "https://controlplane.tailscale.com",
            "ephemeral": false,
            "hostname": "my-server",
            "exit_node": "100.101.102.103",
            "exit_node_allow_lan_access": false,
            "advertise_routes": ["192.168.1.0/24"],
            "advertise_exit_node": false,
            "udp_timeout": "5m"
        }
    ],
    "inbounds": [
        {
            "type": "tun",
            "tag": "tun-in",
            "interface_name": "tun0",
            "address": ["172.18.0.1/30", "fdfe:dcba:9876::1/126"],
            "mtu": 1492,
            "auto_route": true,
            "strict_route": true,
            "route_address": ["0.0.0.0/1", "128.0.0.0/1", "::/1", "8000::/1"],
            "route_exclude_address": ["192.168.0.0/16", "fc00::/7"],
            "endpoint_independent_nat": false,
            "stack": "system",
            "exclude_package": [
                "com.android.captiveportallogin",
                "com.dianchou.dcw",
                "com.jaemobird.mutongji",
                "com.MobileTicket",
                "com.tencent.mm",
                "com.tencent.mobileqq",
                "com.miui.cloudservice",
                "com.miui.weather2",
                "com.xiaomi.market",
                "cn.gov.tax.its"
            ],
            "platform": {
                "http_proxy": {
                    "enabled": true,
                    "server": "127.0.0.1",
                    "server_port": 7890
                }
            }
        },
        {
            "type": "mixed",
            "tag": "mixed-in",
            "listen": "0.0.0.0",
            "listen_port": 7890,
            "users": []
        }
    ],
    "outbounds": [
        {
            "tag": "proxy",
            "type": "selector",
            "outbounds": ["auto", "direct", "{all}"],
            "default": "auto",
            "interrupt_exist_connections": true
        },
        {
            "tag": "auto",
            "type": "urltest",
            "outbounds": ["{all}"],
            "filter": [{"action": "exclude", "keywords": ["ÁΩëÁ´ô|Âú∞ÂùÄ|Ââ©‰Ωô|ËøáÊúü|Êó∂Èó¥|ÊúâÊïà"]}],
            "url": "https://www.gstatic.com/generate_204",
            "interval": "30s",
            "tolerance": 50,
            "idle_timeout": "30m",
            "interrupt_exist_connections": true
        },
        {
            "tag": "Others",
            "type": "selector",
            "outbounds": ["{all}", "proxy"],
            "filter": [{"action": "exclude", "keywords": ["üá≠üá∞|HK|hk|È¶ôÊ∏Ø|È¶ô|üáπüáº|TW|tw|Âè∞Êπæ|Âè∞|üá∏üá¨|SG|sg|Êñ∞Âä†Âù°|ÁãÆ|üáØüáµ|JP|jp|Êó•Êú¨|Êó•|üá∫üá∏|US|us|ÁæéÂõΩ|Áæé|üá∞üá∑|KR|kr|Èü©ÂõΩ"]}]
        },
        {
            "type": "direct",
            "tag": "direct"
        }
    ],
    "route": {
        "rules": [
            {
                "inbound": ["tun-in","mixed-in"],
                "action": "sniff",
                "timeout": "1s"
            },
            {
                "type": "logical",
                "mode": "or",
                "rules": [
                    {
                        "port": 53
                    },
                    {
                         "protocol": "dns"
                    }
                ],               
                "action": "hijack-dns"
            },
            {
                "ip_is_private": true,
                "action": "route",
                "outbound": "direct"
            },
            {
                "clash_mode": "Direct",
                "action": "route",
                "outbound": "direct"
            },
            {
                "clash_mode": "Global",
                "action": "route",
                "outbound": "proxy"
            },
            {
                "type": "logical",
                "mode": "or",
                "rules": [
                    {
                        "port": 853
                    },
                    {
                        "network": "udp",
                        "port": 443,
                        "domain_suffix": {
                            [".google.com", ".youtube.com"],
                            "invert": true
                        }
                    },
                    {
                        "protocol": "stun"
                    }
                ],
                "action": "reject"
            },
            {
                "rule_set": "geosite-category-ads-all",
                "action": "reject",
                "method": "default",
                "no_drop": false
            },
            {
                "rule_set": ["geoip-cn", "geosite-geolocation-cn"],
                "action": "route",
                "outbound": "direct"
            }
        ],
        "default_domain_resolver":
        {
            "server": "dns_resolver",
            "rewrite_tll": 60,
            "client_subnet": "1.1.1.1/32"
        },
        "final": "proxy",
        "auto_detect_interface": true,
        "rule_set": [
            {
                "tag": "geosite-category-ads-all",
                "type": "remote",
                "format": "binary",
                "url": "https://github.com/lyc8503/sing-box-rules/raw/refs/heads/rule-set-geosite/geosite-category-ads-all.srs",
                "download_detour": "auto",
                "update_interval": "1d"
            },
            {
                "tag": "geosite-geolocation-cn",
                "type": "remote",
                "format": "binary",
                "url": "https://github.com/lyc8503/sing-box-rules/raw/refs/heads/rule-set-geosite/geosite-geolocation-cn.srs",
                "download_detour": "auto",
                "update_interval": "1d"
            },
            {
                "tag": "geosite-geolocation-!cn",
                "type": "remote",
                "format": "binary",
                "url": "https://github.com/lyc8503/sing-box-rules/raw/refs/heads/rule-set-geosite/geosite-geolocation-!cn.srs",
                "download_detour": "auto",
                "update_interval": "1d"
            },
            {
                "tag": "geoip-cn",
                "type": "remote",
                "format": "binary",
                "url": "https://github.com/lyc8503/sing-box-rules/raw/refs/heads/rule-set-geoip/geoip-cn.srs",
                "download_detour": "auto",
                "update_interval": "1d"
            }
        ]
    },
    "experimental": {
        "cache_file": {
            "enabled": true,
            "path": "cache.db",
            "store_fakeip": true,
            "store_rdrc": true,
            "rdrc_timeout": "7d"
        },
        "clash_api": {
            "external_controller": "127.0.0.1:9090",
            "external_ui": "ui",
            "external_ui_download_url": "https://github.com/MetaCubeX/metacubexd/archive/refs/heads/gh-pages.zip",
            "external_ui_download_detour": "direct",
            "secret": "",
            "default_mode": "Rule"
        }
    }
}
