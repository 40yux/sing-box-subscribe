                "query_type": ["A", "AAAA"],
                "server": "dns_fakeip",
                "disable_cache": false,
                "rewrite_ttl": 1
            }
        ],
        "final": "dns_proxy",
        "strategy": "prefer_ipv4",
        "disable_cache": false,
        "disable_expire": false,
        "independent_cache": false,
        "reverse_mapping": false,
        "client_subnet": "114.114.114.114/24",
